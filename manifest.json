{
  "id": "wakatime",
  "name": "WakaTime",
  "version": "1.0.0",
  "main": "index.html",
  "host": [
    {
      "app": "PS",
      "minVersion": "26.9"
    }
  ],
  "manifestVersion": 5,
  "entrypoints": [
    {
      "type": "panel",
      "id": "vanilla",
      "minimumSize": { "width": 240, "height": 147 },
      "maximumSize": { "width": 400, "height": 147 },
      "preferredDockedSize": { "width": 240, "height": 147 },
      "preferredFloatingSize": { "width": 240, "height": 147 },
      "icons": [
        {
          "width": 32,
          "height": 32,
          "path": "icons/panel/dark.png",
          "scale": [1, 2],
          "theme": ["dark", "darkest"],
          "species": ["generic"]
        },
        {
          "width": 32,
          "height": 32,
          "path": "icons/panel/light.png",
          "scale": [1, 2],
          "theme": ["lightest", "light"],
          "species": ["generic"]
        }
      ],
      "label": {
        "default": "WakaTime"
      }
    }
  ],
  "icons": [
    {
      "width": 23,
      "height": 23,
      "path": "icons/plugin/dark.svg",
      "theme": ["darkest", "dark", "medium"]
    },
    {
      "width": 23,
      "height": 23,
      "path": "icons/plugin/light.svg",
      "theme": ["lightest", "light"]
    }
  ],
  "featureFlags": {
    "enableAlerts": true
  },
  "requiredPermissions": {
    "launchProcess": {
      "schemes": [ "https", "slack" ],
      "extensions": [ ".pdf", ".xd", ".psd" ]
    }
  }
}
